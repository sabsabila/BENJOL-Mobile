<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alifadepe.android_example">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:name=".Starter"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/BenjolTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".view.PaymentActivity" />
        <activity android:name=".view.ChatBridgeActivity" />
        <activity android:name=".view.ListBookActivity" />
        <activity android:name=".view.ListBengkelActivity" />
        <activity android:name=".view.RegisterActivity" />
        <activity android:name=".view.DashboardActivity" />
        <activity android:name=".view.ProfileActivity" />
        <activity android:name=".view.EditProfileActivity" />
        <activity android:name=".view.PickupActivity" />
        <activity android:name=".view.SparepartActivity" />
        <activity android:name=".view.SparepartBengkelActivity"/>
        <activity android:name=".view.MotorcycleActivity" />
        <activity android:name=".view.EditMotorcycleActivity" />
        <activity android:name=".view.ProgressServiceActivity" />
        <activity android:name=".view.DetailBengkelActivity" />
        <activity android:name=".view.BookingActivity" />
        <activity android:name=".view.LoginActivity"/>
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>